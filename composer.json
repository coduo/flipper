{
    "name": "coduo/flipper",
    "type": "library",
    "keywords": ["php", "humanize", "humanizer"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Dąbrowski",
            "email": "dabrowski@brillante.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*",
        "bossa/phpspec2-expect": "dev-master",
        "behat/behat": "3.0.*",
        "coduo/phpspec-prepare-extension": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {"": "src", "Coduo\\Tests": "tests/", "Features\\": "features/bootstrap/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
